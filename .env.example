# This is an example .env file. Copy this to .env and fill in your actual API keys.
# DO NOT commit your actual .env file to version control.

# --- Core API Keys ---
# Used by NextChat directly (in nextchat/.env.local) and by stonk_research_mcp_server (via nextchat/app/mcp/mcp_config.json)
OPENAI_API_KEY="YOUR_OPENAI_API_KEY_HERE"

# Used by wealthfront_mcp_server (via nextchat/app/mcp/mcp_config.json)
WEALTH_ARC_API_KEY="YOUR_WEALTH_ARC_API_KEY_HERE"

# --- Stock Research Agent Specific Keys (used by stonk_research_mcp_server via nextchat/app/mcp/mcp_config.json) ---
# These are optional but highly recommended for full functionality of the stock research agent.
FINNHUB_API_KEY="YOUR_FINNHUB_API_KEY_HERE"
ALPHAVANTAGE_API_KEY="YOUR_ALPHAVANTAGE_API_KEY_HERE"
FRED_API_KEY="YOUR_FRED_API_KEY_HERE"
EIA_API_KEY="YOUR_EIA_API_KEY_HERE"
NEWSAPI_API_KEY="YOUR_NEWSAPI_API_KEY_HERE"

# --- Other Configurations (if any) ---
# Example:
# SOME_OTHER_SERVICE_KEY="YOUR_OTHER_KEY"
